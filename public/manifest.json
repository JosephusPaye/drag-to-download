{
  "name": "Drag to download",
  "description": "Drag files and images from the browser to the desktop to download.",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
